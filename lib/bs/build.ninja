bs_package_flags = -bs-package-map piggybowl
src_root_dir = /home/adesso/Desktop/projects/cockpit
bsc = /home/adesso/Desktop/projects/cockpit/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/adesso/Desktop/projects/cockpit/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +5
bsc_flags = -bs-suffix -nostdlib -I '/home/adesso/Desktop/projects/cockpit/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always
ppx_flags = 
bs_package_includes = -I /home/adesso/Desktop/projects/cockpit/node_modules/reason-react/lib/ocaml -I /home/adesso/Desktop/projects/cockpit/node_modules/bs-webapi/lib/ocaml
bs_package_dev_includes = 
namespace = -ns Piggybowl
bsb_dir_group = 0
refmt = /home/adesso/Desktop/projects/cockpit/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = -ppx '/home/adesso/Desktop/projects/cockpit/node_modules/bs-platform/lib/reactjs_jsx_ppx_2.exe'
refmt_flags = --print binary
bsc_lib_includes = -I . -I src/NavBulk/views -I src/App
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/App/AppContainer.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/App/AppContainer.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/App/AppContainer.mlast.d : build_deps src/App/AppContainer.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/App/AppContainer-Piggybowl.cmj |  $src_root_dir/src/App/AppContainer.bs.js src/App/AppContainer-Piggybowl.cmi : build_cmj_cmi src/App/AppContainer.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src/App
build src/NavBulk/views/NavBulkView.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/NavBulk/views/NavBulkView.re
build src/NavBulk/views/NavBulkView.mlast.d : build_deps src/NavBulk/views/NavBulkView.mlast
build src/NavBulk/views/NavBulkView-Piggybowl.cmj |  $src_root_dir/src/NavBulk/views/NavBulkView.bs.js src/NavBulk/views/NavBulkView-Piggybowl.cmi : build_cmj_cmi src/NavBulk/views/NavBulkView.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src/NavBulk/views
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build Piggybowl.cmi : build_package Piggybowl.mlmap
build build.ninja : phony  ||  Piggybowl.cmi src/NavBulk/views/NavBulkView.mlast.d src/App/AppContainer.mlast.d
